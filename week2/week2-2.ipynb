    "import re\n",
    "\n",
    "filename = 'regex_sum_377411.txt'\n",
    "with open(filename) as f:\n",
    "    content = f.readlines()\n",
    "    \n",
    "lines = [line.strip() for line in content]\n",
    "\n",
    "result_list = []\n",
    "\n",
    "for line in lines:\n",
    "    result_list.extend(re.findall('[0-9]+', line))\n",
    "    \n",
    "print(sum(list(map(int, result_list))))"
   ]
  }
